<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.spartajet.fxboot.demo.controller.LoginController">
   <children>
      <Button layoutX="184.0" layoutY="200.0" mnemonicParsing="false" onAction="#submit" text="Login" />
      <TextField fx:id="name" layoutX="156.0" layoutY="87.0" promptText="username" />
      <PasswordField fx:id="pwd" layoutX="156.0" layoutY="133.0" promptText="password" />
      <TextField fx:id="user" layoutX="374.0" layoutY="133.0" />
      <Button layoutX="402.0" layoutY="200.0" mnemonicParsing="false" onAction="#getUser" text="check" />
   </children>
</AnchorPane>
